repeat
    task.wait()
until game:IsLoaded()

local Env = getgenv()
local Global = _G
local Set = setmetatable({},{__index = function(A, B) return game:GetService(B)end})
if getgenv then
    Env.Get = Set
elseif not getgenv then
    Global.Get = Set
end 
do
    for i,v in pairs(game:GetChildren()) do
        Env[v.Name] = v
        Env["VirtualUser"] = Get.VirtualUser
    end 
end
